	Journal = {Journal of Tropical Ecology},
	Mendeley-Groups = {Dissertation},
	Number = {3},
	Pages = {243--253},
	Pmid = {230499400001},
	Title = {{Linking seed fate to natural dispersal patterns: factors affecting predation and scatter-hoarding of Virola calophylla seeds in Peru}},
