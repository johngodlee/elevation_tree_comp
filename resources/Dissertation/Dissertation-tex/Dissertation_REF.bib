	Isbn = {0006-3606},
	Journal = {Biotropica},
	Keywords = {Brosimum alicastrum,COMMUNITIES,DENSITY-DEPENDENCE,Matisia cordata,NEOTROPICAL RAIN-FOREST,Panama,Peru,Plant,Pouteria reticulata,RECRUITMENT,SEED SIZE,below-ground competition,competitive irrelevance,diversity,gaps,light,maintenance of biodiversity,resource competition,zone of influence},
