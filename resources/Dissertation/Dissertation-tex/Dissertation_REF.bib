	Isbn = {0176-1617},
	Issn = {0176-1617},
	Journal = {Journal of plant physiology},
	Keywords = {0,00,0176-1617,03,03-271,15,160,abbreviations,calmit,chl,chlorophyll,chlorophylls,e-mail corresponding author,edu,gitelson,leaf optics,near infra red,nir,non-destructive assessment,reflectance,rmse,root mean square error,unl},
