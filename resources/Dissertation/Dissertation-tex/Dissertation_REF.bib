	Isbn = {0028-646X},
	Issn = {0028646X},
	Journal = {New Phytologist},
	Keywords = {Acclimation,Functional group,Global antibodies,Photosynthesis,Protein abundance,Respiration,Temperature},
	Mendeley-Groups = {Dissertation,Dissertation/Acclimation and range change},
	Number = {2},
	Pages = {375--389},
