	Isbn = {0005-2728},
	Issn = {00052728},
	Journal = {Biochimica et Biophysica Acta - Bioenergetics},
	Keywords = {Oxygen evolution,Photosystem II,S-states,UV-damage},
	Mendeley-Groups = {Dissertation},
	Number = {6},
	Pages = {876--882},
	Pmid = {17207455},
	Title = {{The sensitivity of Photosystem II to damage by UV-B radiation depends on the oxidation state of the water-splitting complex}},
