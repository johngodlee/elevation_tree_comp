	Isbn = {1100-9233},
	Issn = {11009233},
	Journal = {Journal of Vegetation Science},
	Keywords = {BIODIVERSITY,British Columbia,CANONICAL CORRESPONDENCE-ANALYSIS,COMMUNITIES,DISTURBANCE,DIVERSITY,GRASSLAND,HUMPED RELATIONSHIP,INVASIBILITY,OAK SAVANNA,Quercus garryana,SPECIES RICHNESS,biodiversity,competition,conservation,facilitation,oak savanna,refuge,soil depth},
	Mendeley-Groups = {Dissertation},
