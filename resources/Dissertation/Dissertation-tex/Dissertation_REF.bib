	File = {:Users/johngodlee/Dropbox/Kitayama{\_}et{\_}al-2002-Journal{\_}of{\_}Ecology.pdf:pdf},
	Isbn = {1365-2745},
	Issn = {00220477},
	Journal = {Journal of Ecology},
	Keywords = {Air temperature,Carbon sequestration,Foliar nutrients,Leaf area index,Nutrient and quantum use efficiency},
	Mendeley-Groups = {Dissertation},
	Number = {1},
	Pages = {37--51},
	Title = {{Ecosystem structure and productivity of tropical rain forests along altitudinal gradients with contrasting soil phosphorus pools on Mount Kinabalu,Borneo}},
