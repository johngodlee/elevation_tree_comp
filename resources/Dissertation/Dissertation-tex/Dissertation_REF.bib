	Issn = {1559-2316},
	Journal = {Plant Signaling {\&} Behavior},
	Keywords = {biomechanics,leaf anatomy,phenotypic plasticity,plant architecture,signal transduction thigmomorphogen-},
	Mendeley-Groups = {Dissertation},
