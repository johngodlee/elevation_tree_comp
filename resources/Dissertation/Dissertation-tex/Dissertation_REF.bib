	File = {:Users/johngodlee/Downloads/art{\%}3A10.1007{\%}2Fs00442-009-1526-3.pdf:pdf},
	Isbn = {0029-8549},
	Issn = {00298549},
	Journal = {Oecologia},
	Keywords = {Climate change,Neophyte,Plant-soil feedback,Range shift,Warming},
	Mendeley-Groups = {Dissertation},
	Number = {4},
