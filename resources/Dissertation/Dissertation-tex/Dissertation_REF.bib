	Isbn = {0028-646X},
	Issn = {0028646X},
	Journal = {New Phytologist},
	Keywords = {Adult stature,Functional traits,Growth,Height,Hydraulic conductance,Shade tolerance,Vessels,Wood density},
	Mendeley-Groups = {Dissertation},
