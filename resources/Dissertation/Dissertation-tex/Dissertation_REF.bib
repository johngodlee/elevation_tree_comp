	Doi = {10.5194/bg-11-843-2014},
	File = {:Users/johngodlee/Library/Application Support/Mendeley Desktop/Downloaded/Asner et al. - 2014 - Landscape-scale changes in forest structure and functional traits along an Andes-to-Amazon elevation gradient.pdf:pdf},
	Isbn = {1810-6285},
	Issn = {17264170},
	Journal = {Biogeosciences},
	Mendeley-Groups = {Dissertation,Dissertation/Acclimation and range change},
