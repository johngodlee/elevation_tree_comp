	Isbn = {1523-1739},
	Issn = {08888892},
	Journal = {Conservation Biology},
	Keywords = {Keystone species,Species diversity,Species turnover},
	Mendeley-Groups = {Synoptic Exam},
	Number = {4},
	Pages = {1019--1024},
	Pmid = {8701},
	Title = {{Rare species and ecosystem functioning}},
	Volume = {19},
